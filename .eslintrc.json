{
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "rules": {
    "no-console": "off"
  },
  "extends": [ "plugin:@wordpress/eslint-plugin/recommended" ],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2016,
    "sourceType": "module"
  },
  "globals": {
    "wp": true,
    "jQuery": true
  }
}